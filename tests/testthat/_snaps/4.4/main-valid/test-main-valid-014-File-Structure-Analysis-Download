                                    <div data-spinner-id="spinner-90b83b5671ba939ccdafdd754075f3b6" class="shiny-spinner-output-container shiny-spinner-hideui">
                                      <div class="load-container shiny-spinner-hidden load1">
                                        <div id="spinner-90b83b5671ba939ccdafdd754075f3b6" class="loader">Loading...</div>
                                      </div>
                                      <div style="height:400px" class="shiny-spinner-placeholder"></div>
                                      <div id="sys_comp_summary_selections" class="shiny-html-output"></div>
                                    </div>
                                    <div data-spinner-id="spinner-dfeeb3b902d549ae1579e65008a512d5" class="shiny-spinner-output-container shiny-spinner-hideui">
                                      <div class="load-container shiny-spinner-hidden load1">
                                        <div id="spinner-dfeeb3b902d549ae1579e65008a512d5" class="loader">Loading...</div>
                                      </div>
                                      <div class="shiny-plot-output html-fill-item" id="sys_comp_summary_ui_chart" style="width:100%;height:400px;"></div>
